{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "include": [
    "src/**/*.ts",
    "src/polyfills.ts",
    "./node_modules/ng-study/src/**/*.ts",
    "./node_modules/ng-study/src/polyfills.ts"
  ],
  "exclude": [
